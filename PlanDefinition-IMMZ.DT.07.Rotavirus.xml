  <action>
    <title value="Immunize Patient for Rotavirus - No Doses"/>
    <description value="Provide Rotavirus immunizations – using the &quot;Rotavirus vaccine immunization – NO PREVIOUS&quot; schedule (2-3 scheme depends on product)"/>
    <condition>
      <kind value="applicability"/>
      <expression>
        <description value="&quot;Rotavirus immunization history&quot; = &quot;No-doses&quot;"/>
        <language value="text/cql"/>
        <expression value="Should vaccinate patient for Rotavirus 1st dose"/>
      </expression>
    </condition>
    <condition>
      <kind value="applicability"/>
      <expression>
        <description value="&quot;Current Patient Age in Weeks&quot; &gt;= 6"/>
        <language value="text/cql"/>
        <expression value="Should vaccinate patient for Rotavirus 1st dose"/>
      </expression>
    </condition>
    <condition>
      <kind value="applicability"/>
      <expression>
        <description value="&quot;Current Patient Age in Months&quot; &lt;= 24"/>
        <language value="text/cql"/>
        <expression value="Should vaccinate patient for Rotavirus 1st dose"/>
      </expression>
    </condition>
    <condition>
      <kind value="applicability"/>
      <expression>
        <description value="&quot;Patient has no history of intussusception&quot; = TRUE"/>
        <language value="text/cql"/>
        <expression value="Should vaccinate patient for Rotavirus 1st dose"/>
