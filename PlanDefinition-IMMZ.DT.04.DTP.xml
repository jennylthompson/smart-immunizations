    </coding>
  </jurisdiction>
  <library value="http://fhir.org/guides/who/smart-immunization/Library/IMMZDT04"/>
  <action>
    <title value="Immunize Patient for DTP - No Doses"/>
    <description value="Provide DTP immunization 1st dose – using the &quot;DTP vaccine immunization – NO PREVIOUS&quot; schedule (3 dose scheme) if the patient is the older 6 weeks of age"/>
    <condition>
      <kind value="applicability"/>
      <expression>
        <description value="&quot;DTP vaccine immunization history&quot; = &quot;No-doses&quot;"/>
        <language value="text/cql"/>
        <expression value="Should vaccinate patient with DTP 1st dose"/>
      </expression>
    </condition>
    <condition>
      <kind value="applicability"/>
      <expression>
        <description value="&quot;Current Patient Age in Weeks&quot; &gt;= 6"/>
        <language value="text/cql"/>
        <expression value="Should vaccinate patient with DTP 1st dose"/>
      </expression>
    </condition>
    <definitionCanonical value="http://fhir.org/guides/who/smart-immunization/ActivityDefinition/IMMZ.DT.04.DTP.01"/>
  </action>
  <action>
    <title value="Immunize Patient for DTP - 1 Dose"/>
    <description value="Provide DTP immunization 2nd dose – using the &quot;DTP vaccine immunization – 3 dose scheme&quot; if the patient is  4-8 weeks after their first dose"/>
    <condition>
      <kind value="applicability"/>
      <expression>
        <description value="&quot;DTP vaccine immunization history&quot; = &quot;1 dose&quot;"/>
        <language value="text/cql"/>
        <expression value="Should vaccinate patient for DTP with 2nd dose"/>
      </expression>
    </condition>
    <condition>
      <kind value="applicability"/>
      <expression>
        <description value="&quot;Date last DTP dose given&quot; &gt;= &quot;4 weeks&quot;"/>
        <language value="text/cql"/>
        <expression value="Should vaccinate patient for DTP with 2nd dose"/>
      </expression>
    </condition>
    <definitionCanonical value="http://fhir.org/guides/who/smart-immunization/ActivityDefinition/IMMZ.DT.04.DTP.02"/>
  </action>
  <action>
    <title value="Immunize Patient for DTP - 2 Doses"/>
    <description value="Provide DTP immunizations – using the &quot;DTP vaccine immunization – 3 dose scheme, provide the 3rd dose&quot; if the patient is  4-8 weeks after their first dose"/>
    <condition>
      <kind value="applicability"/>
      <expression>
        <description value="&quot;DTP vaccine immunization history&quot; = &quot;2 doses&quot;"/>
        <language value="text/cql"/>
        <expression value="Should vaccinate patient for DTP with 3rd dose"/>
      </expression>
    </condition>
    <condition>
      <kind value="applicability"/>
      <expression>
        <description value="&quot;Date last DTP dose given&quot; &gt;= &quot;4 weeks&quot;"/>
        <language value="text/cql"/>
        <expression value="Should vaccinate patient for DTP with 3rd dose"/>
      </expression>
    </condition>
    <definitionCanonical value="http://fhir.org/guides/who/smart-immunization/ActivityDefinition/IMMZ.DT.04.DTP.03"/>
  </action>
  <action>
    <title value="Immunize Patient for DTP - Booster 1"/>
    <description value="Provide booster at 12-23 months (DTP-containing vaccine) if patient has completed primary 3 dose scheme"/>
    <condition>
      <kind value="applicability"/>
      <expression>
        <description value="&quot;DTP vaccine immunization history&quot; = &quot;3 doses&quot;"/>
        <language value="text/cql"/>
        <expression value="Should vaccinate patient for DTP with 1st booster"/>
      </expression>
    </condition>
    <condition>
      <kind value="applicability"/>
      <expression>
        <description value="&quot;Current Patient Age in Months&quot; &gt;= 12"/>
        <language value="text/cql"/>
        <expression value="Should vaccinate patient for DTP with 1st booster"/>
      </expression>
    </condition>
    <definitionCanonical value="http://fhir.org/guides/who/smart-immunization/ActivityDefinition/IMMZ.DT.04.DTP.05"/>
  </action>
  <action>
    <title value="Immunize Patient for DTP - Booster 2"/>
    <description value="Provide booster at 4-7 years (Td/DT containing vaccine)  if patient has completed primary 3 dose scheme and 1 booster"/>
    <condition>
      <kind value="applicability"/>
      <expression>
        <description value="&quot;DTP vaccine immunization history&quot; = &quot;4 doses&quot;"/>
        <language value="text/cql"/>
        <expression value="Should vaccinate patient for DTP with 2nd booster"/>
      </expression>
    </condition>
    <condition>
      <kind value="applicability"/>
      <expression>
        <description value="&quot;Current Patient Age in Years&quot; &gt;= 4"/>
        <language value="text/cql"/>
        <expression value="Should vaccinate patient for DTP with 2nd booster"/>
      </expression>
    </condition>
    <definitionCanonical value="http://fhir.org/guides/who/smart-immunization/ActivityDefinition/IMMZ.DT.04.DTP.06"/>
  </action>
  <action>
    <title value="Immunize Patient for DTP - Booster 3"/>
    <description value="Provide booster at 9-15 years (Td containing vaccine), if &gt;7 years use only aP containing vaccine - if patient has completed primary 3 dose scheme and 2 boosters"/>
    <condition>
      <kind value="applicability"/>
      <expression>
        <description value="&quot;DTP vaccine immunization history&quot; = &quot;5 doses&quot;"/>
        <language value="text/cql"/>
